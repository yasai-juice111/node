<div id="contents">
	<div id="sub">
		<div id="studioList">
			<h3>スタジオ一覧</h3>
			<ul>
			</ul>
		</div>
	</div>
	<div id="main">
		<h2 id="studioName">スタジオ名</h2>
		<div id="calendar"></div>
	</div>
	<div class="remodal" data-remodal-id="event" id="modal-area">
	    <!-- モーダルのタイトル -->
	    <h2>登録</h2>
	    <div id="form">
	    	<h3 id="eventDate"></h3>
	    	<p>タイトル</p>
	    	<input type="text">
			<p>開始日時</p>
	    	<input id="eventStartDate" type="date" name="date">
	    	<input id="eventStartTime" type="time" name="example">
	    	<p>終了日時</p>
	    	<input id="eventEndDate" type="date" name="date">
	    	<input id="eventEndTime" type="time" name="example">
	    	<p>タイプ</p>
	    	<input type="radio" name="event_type" value="booking" checked> 予約可能
			<input type="radio" name="event_type" value="rental"> 貸し出し
	    </div>
	    <!-- キャンセルボタン -->
	    <a class="remodal-cancel" href="#" data-remodal-action="cancel">Cancel</a>
	    <!-- OKボタン -->
	    <a class="remodal-confirm" href="#" data-remodal-action="confirm">OK</a>
	</div>
</div>