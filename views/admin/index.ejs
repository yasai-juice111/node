<div id="stage" class="lunch paT0" style="position:relative">

    <h1 class="lunch_title"><%= dateformat(saleDate, 'yyyy年m月dd日') %>のメニュー</h1>
    <ul class="lunch_list_wrapper">
	    <% _.each(userLunchBoxList, function(userLunchBox){ %>
			<li class="lunch_menu_layout">
	            <div class="lunch_menu_image">
	                <img src="<%= userLunchBox.lunchBox.imagePath %>" alt="" width="100%;" height="100%;">
	            </div>
	            <div class="text_layout_s maL0">
	                <p><%= userLunchBox.employeeId %> <%= userLunchBox.company %></p>
	                <p><%= userLunchBox.lunchBox.name %></p>
	                <p>販売場所：<%= userLunchBox.lunchBox.store %></p>
	            </div>
        		<a href="/admin/detail?id=<%= userLunchBox.id %>" class="order_button">詳細</a>
	        </li>
	    <% }); %>
    </ul>
</div>